{
    "name": "rectitude-open/filament-system-settings",
    "description": "This is my package filament-system-settings",
    "keywords": [
        "filament",
        "filament plugin"
    ],
    "homepage": "https://github.com/rectitude-open/filament-system-settings",
    "support": {
        "issues": "https://github.com/rectitude-open/filament-system-settings/issues",
        "source": "https://github.com/rectitude-open/filament-system-settings"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rectitude Open",
            "email": "open@rectitude.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "filament/spatie-laravel-settings-plugin": "*",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "RectitudeOpen\\FilamentSystemSettings\\": "src/",
            "RectitudeOpen\\FilamentSystemSettings\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RectitudeOpen\\FilamentSystemSettings\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RectitudeOpen\\FilamentSystemSettings\\FilamentSystemSettingsServiceProvider"
            ],
            "aliases": {
                "FilamentSystemSettings": "RectitudeOpen\\FilamentSystemSettings\\Facades\\FilamentSystemSettings"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
